{
  "name": "@fantasticfour/world-postgres-upstash",
  "version": "1.0.2",
  "description": "World implementation using PostgreSQL for storage and Upstash QStash for queue management - works with any PostgreSQL provider (Neon, Supabase, RDS, etc.)",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "world-postgres-upstash-setup": "./bin/setup.js"
  },
  "files": [
    "dist",
    "bin",
    "migrations"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinnymac/worlds.git",
    "directory": "packages/world-postgres-upstash"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "default": "./dist/cli.js"
    },
    "./migrations/*.sql": "./migrations/*.sql"
  },
  "scripts": {
    "build": "tsc && chmod +x bin/setup.js",
    "dev": "tsc --watch",
    "clean": "tsc --build --clean && rm -rf dist",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:push": "node dist/cli.js",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@upstash/qstash": "^2.7.38",
    "@workflow/errors": "catalog:",
    "@workflow/world": "catalog:",
    "@workflow/world-local": "catalog:",
    "cbor-x": "catalog:",
    "drizzle-orm": "catalog:",
    "postgres": "catalog:",
    "ulid": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "catalog:",
    "@types/node": "catalog:",
    "@workflow/world-testing": "catalog:",
    "dotenv": "catalog:",
    "drizzle-kit": "catalog:",
    "testcontainers": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": [
    "workflow",
    "postgres",
    "postgresql",
    "upstash",
    "qstash",
    "neon",
    "supabase",
    "durable-functions"
  ],
  "author": "vinnymac",
  "packageManager": "pnpm@10.23.0",
  "volta": {
    "extends": "../../package.json"
  }
}
